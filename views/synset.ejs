<html>
	<head>
		<title><%= title %></title>
		</head>
		<body>
			<h1><a href="/surface/<%= synset.term %>.html"><%= synset.term %></a></h1>
			<p><a href="<%= synset.synset_ref %>.html"><%= synset.synset_id %></a> <%= synset.fos %><ul>
<% for (var i=0; i<synset.glosses.length; i++) { %>
				<li><%= synset.glosses[i] %></li>
<% } %>
			</ul></p>
<% if (synset.uses) { %>
			<p>uses<ul>
  <% for (var i=0; i<synset.uses.length; i++) { %>
				<li><%= synset.uses[i] %></li>
  <% } %>
			</ul><p>
<% } %>
<% if (synset.sent_frames) { %>
			<p>sent_frames<ul>
  <% for (var i=0; i<synset.sent_frames.length; i++) { %>
				<li>&quot;<%= synset.sent_frames[i][0] + 'doe' + synset.sent_frames[i][1] %>&quot (<%= synset.term %>)</li>
  <% } %>
			</ul><p>
<% } %>
			<p>words<ol>
<% for (var i=0; i<synset.words.length; i++) { %>
				<li><a href="<%= synset.words[i].word_ref %>.html"><%= synset.words[i].surface %></a></li>
<% } %>
			</ol></p>
<!--
, 'attribute_of' - removed because it is always populated in both directions.
-->
<% var attribs = [ 'hypernyms', 'hyponyms', 'attributes', 'causes', 'caused-by', 'entails', 'entailed-by', 'instance-of', 'has-instances', 'member-of', 'has-members', 'part-of', 'has-parts', 'substance-of', 'has-substances', 'has-satellites', 'satellite-of', 'topics', 'topic-of', 'regions', 'region-of', 'uses', 'usage-of', 'used-by', 'agent', 'agent-of', 'body-part', 'body-part-of', 'by-means-of', 'mechanism-for', 'destination', 'destination-of', 'event', 'event-of', 'instrument', 'instrument-of', 'location', 'location-of', 'material', 'material-of', 'property', 'property-of', 'result', 'result-of', 'state', 'state-of', 'undergoer', 'undergoes', 'vehicle', 'vehicle-of' ] %>
<% for (var i=0; i<attribs.length; i++) { %>
  <% if (synset[attribs[i]] !== undefined) { %>			<p><%= attribs[i] %><ul>
    <% for (var j=0; j<synset[attribs[i]].length; j++) { %>
      <% if (synset[attribs[i]][j].type === 'word') { %>
				<li><a href="<%= synset[attribs[i]][j].word_ref %>.html"><%= synset[attribs[i]][j].surface %></a> - <%= synset[attribs[i]][j].fos %>, <%= synset[attribs[i]][j].gloss %></li>
      <% } else { %>
				<li><a href="<%= synset[attribs[i]][j].synset_ref %>.html"><%= synset[attribs[i]][j].term %></a> - <%= synset[attribs[i]][j].fos %>, <%= synset[attribs[i]][j].gloss %></li>
      <% } %>
    <% } %>
			</ul></p>
  <% } %>
<% } %>
	</body>
</html>
